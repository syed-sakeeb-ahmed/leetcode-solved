class Solution {
public:
    int minimumDeleteSum(string s1, string s2) {
        int m = s1.length();
        int n = s2.length();
        vector<vector<int>> r(m + 1, vector<int>(n + 1, 0));
        r[0][0] = 0;
        for (int i = 1; i < m + 1; i++) {
            r[i][0] = r[i - 1][0] + s1[i - 1];
        }
        for (int j = 1; j < n + 1; j++) {
            r[0][j] = r[0][j - 1] + s2[j - 1];
        }

        for (int i = 1; i < m + 1; i++) {
            for (int j = 1; j < n + 1; j++) {
                if (s1[i - 1] == s2[j - 1]) r[i][j] = r[i - 1][j - 1];
                else {
                    r[i][j] = min(s1[i - 1] + r[i -1][j], s2[j - 1] + r[i][j-1]);
                }
            }
        }

        return r[m][n];
    }
};