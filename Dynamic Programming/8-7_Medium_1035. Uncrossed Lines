class Solution {
public:
    int maxUncrossedLines(vector<int>& nums1, vector<int>& nums2) {
        int m = nums1.size();
        int n = nums2.size();

        vector<vector<int>> r(m + 1, vector<int>(n + 1, 0));

        for (int i = 1; i < m + 1; i++) {
            for (int j = 1; j < n + 1; j++) {
                if (nums1[i - 1] == nums2[j - 1]) {
                    r[i][j] = 1 + r[i - 1][j - 1];
                }
                else {
                    r[i][j] = max(r[i][j - 1], r[i - 1][j]);
                }
            }
        }
        
        return r[m][n];
    }
};