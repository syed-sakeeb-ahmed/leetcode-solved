class Solution {
public:
    int uniquePaths(int m, int n) {
        vector<vector<int>> r(m, vector<int>(n));
        r[0][0] = 1;
        for (int i = 0; i < m; i++) {
            for (int j = 0; j < n; j++) {
                int left = (j - 1 >= 0) ? r[i][j - 1] : 0;
                int upper = (i - 1 >= 0) ? r[i - 1][j] : 0;
                r[i][j] = max(left + upper, r[i][j]);
            }
        }
        return r[m - 1][n - 1];
    }
};