class Solution {
public:
    int rob(vector<int>& nums) {
        if (nums.size() == 1) return nums[0];
        vector<int> r(nums.size(), -1);
        r[0] = nums[0];
        r[1] = max(nums[0], nums[1]);
        for (int i = 2; i < nums.size(); i++) {
            r[i] = max(nums[i] + r[i - 2], r[i - 1]);
        }
        return r[nums.size() - 1];
    }
};