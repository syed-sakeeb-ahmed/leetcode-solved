#include <bits/stdc++.h>
using namespace std;


vector<int> rd = {0, -1, -1,-1,0,1,1,1};
vector<int> cd = {-1, -1, 0,1,1,1,0, -1};

void floodfill(int i, int j, vector<vector<int>>& visited, vector<vector<char>>& grid) {
    if (i < 0 || i > visited.size() - 1 || j < 0 || j > visited.size() - 1) return;
    if (visited[i][j] || grid[i][j] == '0') return;
    visited[i][j] = 1;
    for (int k = 0; k < 8; k++) {
        int r = rd[k];
        int c = cd[k];
        floodfill(i + r, j + c, visited,grid);
    }
}

int main() {
    int R;
    int test = 1;
    while (scanf("%d ", &R) == 1) {
        int count = 0;
        vector<vector<char>> grid(R, vector<char>(R, '0'));
        vector<vector<int>> visited(R,vector<int>(R, 0));
        for (int i = 0; i < R; i++) {
            for (int j = 0; j < R; j++) {
                scanf("%c ", &grid[i][j]);
            }
        }
        
        for (int i = 0; i < R; i++) {
            for (int j = 0; j < R; j++) {
                if (!visited[i][j] && grid[i][j] == '1') {
                    floodfill(i,j,visited,grid);
                    count++;
                }
            }
        }
        
        printf("Image number %d contains %d war eagles.\n", test++, count);
    }
    return 0;
}