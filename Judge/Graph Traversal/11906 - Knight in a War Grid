#include <stdio.h>
#include <bits/stdc++.h>
using namespace std;

int R,C;
int even,odd;
int dir[2] = {};
unordered_set<string> visited;
unordered_set<string> water;
void dfs(int x, int y) {
    int count = 0;
    visited.insert(to_string(x) + ":" + to_string(y));
    for (int k = 0; k < 2; k++) {
        for (int i = -1; i <= 1; i+=2) {
        for (int j = -1; j<=1; j+=2) {
            int nextX = x + dir[k^0] * i;
            int nextY = y + dir[k^1] * j;
            if (nextX < 0 || nextX > R - 1 || nextY < 0 || nextY > C - 1) continue;
            string nextPos = to_string(nextX) + ":" + to_string(nextY);
            if (!visited.count(nextPos) && !water.count(nextPos)) {
                dfs(nextX,nextY);
            }
            if(!water.count(nextPos)) count++;
        }
    }
    }
    if (dir[0] == dir[1] || dir[0] == 0 || dir[1] == 0) count /= 2;
    if (count % 2 == 0) even++;
    else odd++;
}

int main() {
    int t;
    int pepsi = 1;
    scanf("%d", &t);
    while (t--) {
        even = odd = 0;
        water.clear();visited.clear();
        scanf("%d %d %d %d ", &R, &C, &dir[0], &dir[1]);
        
        int W;
        scanf("%d ", &W);
        while(W--) {
            int x,y;
            scanf("%d %d", &x, &y);
            water.insert(to_string(x) + ":" + to_string(y));
        }
        dfs(0,0);
        printf("Case %d: %d %d\n", pepsi++, even, odd);
        
    }
}