class Solution {
public:
    vector<int> topKFrequent(vector<int>& nums, int k) {
        priority_queue<tuple<int,int>> pq;
        unordered_map<int,int> mp;
        for (int i = 0; i < nums.size(); i++) {
            mp[nums[i]]++;
        }
        for (auto item : mp) {
            pq.push({item.second,item.first});
        }
        vector<int> res;
        for (int i = 0; i < k; i++) {
            int x,y;
            tie(x,y) = pq.top(); pq.pop();
            res.push_back(y);
        }
        return res;
    }
};