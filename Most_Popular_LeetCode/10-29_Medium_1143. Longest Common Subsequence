class Solution {
public:
    int longestCommonSubsequence(string text1, string text2) {
        int m = text1.length();
        int n = text2.length();
        vector<vector<int>> res(m + 1,vector<int>(n + 1, 0));
        for (int i = 1; i <= m; i++) {
            for (int j = 1; j <= n; j++) {
                if (text1[i-1] == text2[j-1]) {
                    res[i][j] = 1 + res[i-1][j-1];
                }
                else {
                    res[i][j] = max(res[i-1][j], res[i][j-1]);
                }
            }
        }
        return res[m][n];
    }
};