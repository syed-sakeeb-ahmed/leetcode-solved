class Solution {
unordered_map<char, int> c_mp = {
    {'I', 1},
    {'V', 5},
    {'X', 10},
    {'L', 50},
    {'C', 100},
    {'D', 500},
    {'M', 1000}
};
unordered_map<string, int> s_mp = {
    {"IV", 4},
    {"IX", 9},
    {"XL", 40},
    {"XC", 90},
    {"CD", 400},
    {"CM", 900}
};
public:
    int romanToInt(string s) {
        int item = 0;
        int i = 0;
        while (i < s.length()) {
            char c = s[i];
            if (c == 'I' || c == 'X' || c == 'C') {
                if (i + 1 < s.length()) {
                    string temp = s.substr(i, 2);
                    if (s_mp.count(temp)) {
                        item += s_mp[temp];
                        i += 2;
                        continue;
                    }
                }
            }
            item += c_mp[c];
            i++;
        }
        return item;
    }
};