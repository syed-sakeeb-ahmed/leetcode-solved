class Solution {
public:
    bool canJump(vector<int>& nums) {
        int n = nums.size();
        vector<bool> temp(n, false);
        temp[n -1] = true;
        int left = n-1;
        for (int i = n - 1; i >= 0; i--) {
            int right = min(n-1, nums[i] + i);
            if (right >= left) {
                temp[i] = true;
                left = i;
            }
            else temp[i] = false;
        }
        return temp[0];
    }
};